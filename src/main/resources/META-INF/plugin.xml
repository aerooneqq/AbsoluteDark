<idea-plugin>
    <id>AeroThemes</id>
    <version>1.0.0</version>
    <name>AeroThemes</name>
    <category>UI</category>
    <vendor email="aerooneq@yandex.ru">Aero</vendor>

    <idea-version since-build="241" until-build="243.*"/>
    <description>>Just themes for fun. Now plugin contains the following themes: AbsoluteDark</description>
    <change-notes>Initial version</change-notes>
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <bundledColorScheme id="AbsoluteDark" path="/theme/colorSchemes/AbsoluteDark" />
        <themeProvider id="AbsoluteDark" path="/theme/AbsoluteDark.theme.json" targetUi="new"/>
    </extensions>
</idea-plugin>