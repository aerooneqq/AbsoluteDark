      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "hoverBackground": "$selectionBackgroundMuted",
      "hoverInactiveBackground": "$selectionBackgroundMuted",
      "modifiedItemForeground": "$linkAccented",
      "border": "0,0,0,0"
    },
    "Notification": {
      "background": "$notificationBackground",
      "borderColor": "$notificationBorder",
      "foreground": "$notificationForeground",
      "errorBackground": "$validationErrorBackground",
      "errorBorderColor": "$validationErrorBorder",
      "errorForeground": "$validationErrorForeground",
      "ToolWindow": {
        "errorBackground": "$validationErrorBackground",
        "errorBorderColor": "$validationErrorBorder",
        "errorForeground": "$validationErrorForeground",
        "warningBackground": "$validationWarningBackground",
        "warningBorderColor": "$validationWarningBorder",
        "warningForeground": "$validationWarningForeground",
        "informativeBackground": "$validationInformativeBackground",
        "informativeBorderColor": "$validationInformativeBorder",
        "informativeForeground": "$validationInformativeForeground"
      },
      "MoreButton": {
        "background": "$notificationBackground",
        "innerBorderColor": "$notificationBackground",
        "foreground": "$linkRegular"
      }
    },
    "ValidationTooltip": {
      "errorBackground": "$validationErrorBackground",
      "errorBorderColor": "$validationErrorBorder",
      "warningBackground": "$validationWarningBackground",
      "warningBorderColor": "$validationWarningBorder"
    },
    "Link": {
      "activeForeground": "$linkRegular",
      "hoverForeground": "$linkAccented",
      "pressedForeground": "$linkRegular",
      "visitedForeground": "$linkRegular"
    },
    "Plugins": {
      "background": "$toolwindowBackground",
      "lightSelectionBackground": "$selectionBackgroundMuted",
      "SectionHeader": {
        "background": "$panelBackground",
        "foreground": "$regularForeground"
      },
      "Tab": {
        "selectedBackground": "$toolwindowBackground",
        "selectedForeground": "$regularForeground",
        "hoverBackground": "$tabBackgroundSelected"
      },
      "SearchField": {
        "background": "$textInputBackground",
        "borderColor": "$panelBackground"
      },
      "Button": {
        "installFillBackground": "$buttonFace",
        "installFillForeground": "$regularForeground",
        "installBackground": "$buttonFace",
        "installBorderColor": "$border",
        "installForeground": "$regularForeground",
        "updateBackground": "$buttonFace",
        "updateBorderColor": "$border",
        "updateForeground": "$regularForeground"
      },
      "tagBackground": "$panelBackground",
      "tagForeground": "$regularForeground"
    },
    "Component": {
      "borderColor": "$controlBorder",
      "focusColor": "$controlFocus",
      "focusedBorderColor": "$focusedBorder",
      "disabledBorderColor": "$controlBorder",
      "errorFocusColor": "$validationErrorFocus",
      "inactiveErrorFocusColor": "$validationErrorFocusInactive",
      "warningFocusColor": "$validationWarningFocus",
      "inactiveWarningFocusColor": "$validationWarningFocusInactive",
      "iconColor": "$mutedForeground",
      "hoverIconColor": "$mutedForeground",
      "infoForeground": "$mutedForeground"
    },
    "ToolTip": {
      "background": "$tooltipBackground",
      "foreground": "$regularForeground",
      "borderColor": "$popupBorder",
      "xpaintBorder": "true",
      "infoForeground": "$regularForeground",
      "Actions.background": "$panelBackground",
      "Actions.infoForeground": "$regularForeground"
    },
    "GotItTooltip": {
      "background": "$tooltipBackground",
      "borderColor": "$popupBorder",
      "foreground": "$regularForeground",
      "linkForeground": "$linkRegular",
      "shortcutForeground": "$linkRegular"
    },
    "TextField": {
      "background": "$textInputBackground",
      "disabledBackground": "$textInputBackgroundDisabled",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },
    "FormattedTextField": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },
    "PasswordField": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground"
    },
    "TextArea": {
      "background": "$textInputBackground",
      "foreground": "$regularForeground",
      "inactiveBackground": "$textInputBackground",
      "inactiveForeground": "$regularForeground"
    },
    "ActionButton": {
      "hoverBackground": "$buttonFace",
      "hoverBorderColor": "$controlBorder",
      "pressedBackground": "$pressedBackground",
      "pressedBorderColor": "$pressedBorder",
      "focusedBorderColor": "$focusedBorder"
    },
    "Table": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "stripeColor": "$textInputBackground",
      "gridColor": "$panelBackground",
      "sortIconColor": "$mutedForeground",
      "lightSelectionBackground": "$selectionBackground",
      "lightSelectionForeground": "$selectionForeground",
      "lightSelectionInactiveBackground": "$selectionBackgroundInactive",
      "lightSelectionInactiveForeground": "$selectionForegroundInactive"
    },
    "TableHeader": {
      "background": "$panelBackground",
      "foreground": "$mutedForeground",
      "cellBorder": "0,1,1,1",
      "separatorColor": "$border",
      "bottomSeparatorColor": "$panelBackground"
    },
    "ProgressBar": {
      "trackColor": "#242424",
      "progressColor": "#8F8F8FB0",
      "indeterminateStartColor": "#8F8F8FB0",
      "indeterminateEndColor": "#8F8F8F40",
      "failedColor": "#BA5249B0",
      "failedEndColor": "#BA524940",
      "passedColor": "#6EA35AB0",
      "passedEndColor": "#6EA35A40",
      "selectionForeground": "$regularForeground",
      "selectionBackground": "$regularForeground"
    },
    "Slider": {
      "buttonBorderColor": "$mutedForeground",
      "background": "$panelBackground",
      "trackColor": "$border",
      "tickColor": "$mutedForeground"
    },
    "Counter": {
      "background": "$iconGrayMuted",
      "foreground": "$panelBackground"
    },
    "DragAndDrop": {
      "areaBackground": "#23324D",
      "areaBorderColor": "#23324D",
      "areaForeground": "$regularForeground"
    },
    "Editor": {
      "background": "$toolwindowBackground",
      "foreground": "$regularForeground",
      "shortcutForeground": "$linkRegular"
    },
    "FileColor": {
      "Rose": "#52282e",
      "Orange": "#4f2f1f",
      "Yellow": "#453c1d",
      "Green": "#25401a",
      "Blue": "#283857",
      "Violet": "#472a5e"
    },
    "InplaceRefactoringPopup": {
      "background": "$panelBackground",
      "borderColor": "$border"
    },
    "TabbedPane": {
      "background": "$panelBackground",
      "underlineColor": "$selectionUnderline",
      "disabledUnderlineColor": "$selectionUnderlineInactive",
      "contentAreaColor": "$border",
      "hoverColor": "$tabBackgroundSelected",
      "tabSelectionHeight": 3
    },
    "VersionControl": {},
    "Viewport": {
      "background": "$panelBackground",
      "foreground": "$regularForeground"
    },
    "WelcomeScreen": {
      "background": "$panelBackground",
      "borderColor": "$separator",
      "separatorColor": "$separator",
      "Details.background": "$toolwindowBackground",
      "Projects": {
        "actions.selectionBackground": "$selectionBackground",
        "actions.background": "$panelBackground",
        "background": "$toolwindowBackground",
        "selectionBackground": "$selectionBackground",
        "selectionInactiveBackground": "$selectionBackgroundInactive"
      },
      "SidePanel.background": "$panelBackground"
    },
    "Group": {
      "separatorColor": "$separator"
    },
    "TitlePane": {
      "background": "$panelBackground",
      "inactiveBackground": "$panelBackground",
      "infoForeground": "$mutedForeground",
      "inactiveInfoForeground": "$disabledForeground"
    },
    "ScrollPane": {
      "ScrollPane.background": "$panelBackground",
      "ScrollPane.foreground": "$regularForeground"
    },
    "EditorPane": {
      "inactiveBackground": "$panelBackground",
      "foreground": "$regularForeground"
    },
    "Debugger": {
      "Variables": {
        "valueForeground": "$regularForeground",
        "changedValueForeground": "$validationWarningForeground",
        "modifyingValueForeground": "$notificationForeground",
        "collectingDataForeground": "$mutedForeground",
        "errorMessageForeground": "$validationErrorForeground",
        "evaluatingExpressionForeground": "$notificationForeground",
        "exceptionForeground": "$validationWarningForeground"
      }
    },
    "UnitTests": {
      "UnknownStatus.foreground": "$regularForeground",
      "IgnoredStatus.foreground": "$mutedForeground",
      "SuccessStatus.foreground": "$validationInformativeForeground",
      "InconclusiveStatus.foreground": "$validationWarningForeground",
      "FailedStatus.foreground": "$validationErrorForeground",
      "AbortedStatus.foreground": "$validationErrorForeground",
      "PendingStatus.foreground": "$regularForeground",
      "RunningStatus.foreground": "$regularForeground"
    },
    "NewClass": {
      "Panel.background": "#404040",
      "SearchField.background": "$textInputBackground",
      "separatorWidth": 2
    },
    "DebuggerTabs.selectedBackground": "#242424",
    "Window.border": "0,0,0,0,58585833",
    "DebuggerPopup.borderColor": "$popupBorder",
    "Separator.separatorColor": "$separator",
    "ToolBar.separatorColor": "$separator",
    "SidePanel.background": "$toolwindowBackground",
    "OptionPane.background": "$panelBackground",
    "RunWidget.iconColor": "$iconGrayRegular"
  }
}