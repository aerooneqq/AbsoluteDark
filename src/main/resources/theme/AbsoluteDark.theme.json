{
  "name": "AbsoluteDark",
  "author": "Aero",
  "dark": true,
  "parentTheme": "Darcula",
  "editorScheme": "AbsoluteDark",
  "colors": {
    "$titleBackground": "#0c0c0c",
    "$selectionBackground": "#333333",
    "$selectionBackgroundMuted": "#252525",
    "$selectionUnderline": "#a5a5a5",
    "$selectionBackgroundInactive": "#333333",
    "$selectionUnderlineInactive": "#606469",
    "$selectionForeground": "#EEEEEE",
    "$selectionForegroundInactive": "#EEEEEE",
    "$selectionToolwindowHeader": "#3E424D",
    "$border": "#0c0c0c",
    "$controlBorder": "#0c0c0c",
    "$pressedBackground": "#0c0c0c",
    "$pressedBorder": "#0c0c0c",
    "$separator": "#2c2c2c",
    "$listSeparator": "#232323",
    "$focusedBorder": "#1a1a1a",
    "$controlFocus": "#1a1a1a",
    "$popupBorder": "#1a1a1a",
    "$toolwindowBackground": "#0c0c0c",
    "$panelBackground": "#0c0c0c",
    "$tooltipBackground": "#0c0c0c",
    "$regularForeground": "#a2a2a2",
    "$mutedForeground": "#585858",
    "$disabledForeground": "#383838",
    "$textInputBackground": "#0c0c0c",
    "$textInputBackgroundDisabled": "#2a2a2a",
    "$tabBackgroundSelected": "#1c1c1c",
    "$linkRegular": "#789ADB",
    "$linkAccented": "#5F94FF",
    "$buttonFace": "#0c0c0c",
    "$notificationBackground": "#333C4D",
    "$notificationBorder": "#4B566B",
    "$notificationForeground": "#C4DAFF",
    "$validationErrorBackground": "#592624",
    "$validationErrorBorder": "#753937",
    "$validationErrorForeground": "#FF9794",
    "$validationErrorFocus": "#DB5860A0",
    "$validationErrorFocusInactive": "#DB586060",
    "$validationWarningBackground": "#523911",
    "$validationWarningBorder": "#6C4F20",
    "$validationWarningForeground": "#EDBA68",
    "$validationWarningFocus": "#DE971FA0",
    "$validationWarningFocusInactive": "#DE971F60",
    "$validationInformativeBackground": "#154D4D",
    "$validationInformativeBorder": "#226868",
    "$validationInformativeForeground": "#61EDED",
    "$speedSearchBackground": "#A39254",
    "$speedSearchMatchBackground": "#D4BC64",
    "$searchMatchForeground": "#D4BC64",
    "$iconGrayRegular": "#CDD0D7",
    "$iconGrayMuted": "#CDD0D7B2",
    "$iconGrayDisabled": "#CDD0D766",
    "$bookmarkBackground": "#D9B72B"
